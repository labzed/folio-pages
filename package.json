{
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-preset-react-app": "^6.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-plugin": "^0.10.0",
    "prettier": "^1.15.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "@babel/syntax-dynamic-import"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "clean": "rm -rf ./dist",
    "format": "prettier --write --single-quote --ignore-path .gitignore \"**/*.{js,json,css,md}\""
  }
}
